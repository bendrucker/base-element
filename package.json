{
  "name": "base-element",
  "version": "1.0.1",
  "description": "An element authoring library for creating standalone and performant elements.",
  "main": "index.js",
  "scripts": {
    "test": "standard",
    "ci": "zuul --local 9966 -- test/index.js",
    "start": "budo examples/standalone.js",
    "build": "browserify index.js --standalone BaseElement -o dist/base-element.js"
  },
  "author": "Kyle Robinson Young <kyle@dontkry.com> (http://dontkry.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shama/base-element.git"
  },
  "files": [
    "index.js"
  ],
  "standard": {
    "ignore": [
      "**/dist/**"
    ]
  },
  "keywords": [
    "virtual",
    "dom",
    "element",
    "diff",
    "browser"
  ],
  "devDependencies": {
    "browserify": "^10.0.0",
    "budo": "^4.0.0",
    "standard": "^3.7.2",
    "tap-spec": "^3.0.0",
    "tape": "^4.0.0",
    "zuul": "^3.0.0"
  },
  "dependencies": {
    "virtual-dom": "^2.0.1"
  }
}
